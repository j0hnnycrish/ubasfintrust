# UBAS Financial Trust - Development Environment Variables

# API Configuration
VITE_API_URL=https://ubasfintrust.jcrish4eva.workers.dev/api/v1
VITE_SOCKET_URL=https://ubasfintrust.jcrish4eva.workers.dev

# App Configuration
VITE_APP_NAME=UBAS Financial Trust
VITE_APP_VERSION=1.0.0

# Environment
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_2FA=true
VITE_ENABLE_BIOMETRIC_AUTH=false
VITE_ENABLE_PUSH_NOTIFICATIONS=true

# Banking Configuration
VITE_MINIMUM_DEPOSIT=100
VITE_MINIMUM_WITHDRAWAL=20
VITE_MAXIMUM_DAILY_TRANSFER=50000

# Demo Mode (set to false for production)
VITE_DEMO_MODE=true
VITE_DEMO_ACCOUNT_PREFIX=DEMO

# External Services (Add your API keys here)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_GOOGLE_MAPS_API_KEY=
VITE_GOOGLE_CLIENT_ID=
VITE_FACEBOOK_APP_ID=
